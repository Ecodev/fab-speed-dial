{
    "name": "demo",
    "version": "0.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "ng": "ng",
        "dev": "ng serve",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-lib": "ng build fab-speed-dial && cp -v README.md dist/fab-speed-dial/",
        "build-docs": "git rm -rf --ignore-unmatch docs/ && yarn build-lib && ng build demo --prod --aot --output-path docs --base-href /fab-speed-dial/ && cp docs/index.html docs/404.html && git add docs/ && git commit docs/ -m 'Update documentation'",
        "release": "yarn build-lib && cd dist/fab-speed-dial/ && yarn publish --access public"
    },
    "dependencies": {
        "@angular/animations": "^6.0.0",
        "@angular/cdk": "^6.0.2",
        "@angular/common": "^6.0.0",
        "@angular/compiler": "^6.0.0",
        "@angular/core": "^6.0.0",
        "@angular/forms": "^6.0.0",
        "@angular/http": "^6.0.0",
        "@angular/material": "^6.0.2",
        "@angular/platform-browser": "^6.0.0",
        "@angular/platform-browser-dynamic": "^6.0.0",
        "@angular/router": "^6.0.0",
        "core-js": "^2.5.4",
        "hammerjs": "^2.0.8",
        "puppeteer": "^1.4.0",
        "rxjs": "^6.0.0",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.1",
        "@angular-devkit/build-ng-packagr": "~0.6.1",
        "@angular/cli": "~6.0.1",
        "@angular/compiler-cli": "^6.0.0",
        "@angular/language-service": "^6.0.0",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.2.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^3.0.0-rc.2",
        "protractor": "~5.3.0",
        "ts-node": "~5.0.1",
        "tsickle": ">=0.25.5",
        "tslib": "^1.7.1",
        "tslint": "~5.9.1",
        "typescript": "~2.7.2"
    }
}
