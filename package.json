        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^2.0.0-rc.11",
        "protractor": "~5.3.0",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "2.6.2"
